{
  "schemaVersion": 1,
  "id": "celesteconfig",
  "version": "${version}",
  "name": "Celestial Configuration",
  "description": "Celestial Configuration is a mod that allows one to change the size of the sun and moon to whatever size you wish!",
  "authors": [
    "LudoCrypt",
    "Nekomaster1000"
  ],
  "contact": {
    "homepage": "https://infernalstudios.org/",
    "issues": "https://github.com/infernalstudios/Celestial-Configuration/issues",
    "sources": "https://github.com/infernalstudios/Celestial-Configuration"
  },
  "license": "Apache-2.0",
  "icon": "assets/celesteconfig/icon.png",
  "environment": "client",
  "entrypoints": {
    "main": ["org.infernalstudios.celesteconfig.CelestialConfiguration"],
    "modmenu": ["org.infernalstudios.celesteconfig.config.CelestialConfigModmenu"]
  },
  "mixins": [
    "celesteconfig.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.14.22",
    "fabric-api": "*",
    "minecraft": "~1.20.2",
    "java": ">=17",
    "cloth-config": "*"
  }
}
